{
	"type": "library",
	"name": "youmy001/apine-framework",
	"description": "APIne is a simple to use modular MVC Framework ready for use as a basic RESTful API.",
	"authors": [
		{
			"name": "Tommy Teasdale",
			"email": "tteasdaleroads@gmail.com"
		}
	],
	"support": {
		"email": "tteasdaleroads@gmail.com",
		"issues": "https://github.com/Youmy001/apine-framework/issues",
		"wiki": "https://github.com/Youmy001/apine-framework/wiki",
		"source": "https://github.com/Youmy001/apine-framework"
	},
	"require": {
		"php": ">=7.0",
		"ext-filter": ">=1.0",
		"ext-gd": ">=7.0",
		"ext-hash": ">=1.0",
		"ext-json": ">=1.4",
		"ext-mbstring": ">=7.0",
		"ext-mcrypt": ">=7.0",
		"ext-PDO": ">=7.0",
		"ext-pdo_mysql": ">=7.0",
		"lib-openssl": ">=1.0.1",
		"phpmailer/phpmailer": "^5.2",
		"geoip/geoip": "~1.14",
		"twig/twig": "^2.0",
		"psr/http-message": "^1.0.1",
		"psr/container": "^1.0",
		"psr/http-server-middleware": "^1.0"
    },
	"require-dev": {
		"phpunit/phpunit": "6.5.6"
	},
    "autoload": {
		"files": [
			"src/Autoloader.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Apine\\": "src/"
		}
    },
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		}
	},
	"license": "MIT"
}
